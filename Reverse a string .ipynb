    "def reverse(str):\n",
    "    mylist=[]\n",
    "    for i in range(len(str)-1,-1,-1):\n",
    "        mylist.append(str[i])\n",
    "    return ''.join(mylist)\n",
    "\n",
    "string=input(print('Enter a string'))\n",
    "reverse(string)\n",
